{
    "version": 2,
    "builds": [
        {
            "src": "telegram-connector.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "telegram-connector.py"
        }
    ],
    "env": {
        "PYTHONPATH": "."
    }
} 