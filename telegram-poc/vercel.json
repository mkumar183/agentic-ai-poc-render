{
    "version": 2,
    "builds": [
        {
            "src": "telegram-connector.py",
            "use": "@vercel/python",
            "config": {
                "runtime": "python3.9"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "telegram-connector.py"
        }
    ],
    "env": {
        "PYTHONPATH": "."
    },
    "crons": [
      {
        "path": "/api/telegram-connector",
        "schedule": "30 1,7,13 * * *"
      }
    ]
} 